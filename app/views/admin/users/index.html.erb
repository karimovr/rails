<h1>Пользователи</h1>

<table>
  <thead>
  <tr>
    <th>Читайте</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>

        <td><%= link_to t('common.edit'), edit_admin_user_path(user) %></td>
        <td><%= link_to t('common.delete'), [ :admin, user], method: :delete, data: { confirm: 'Вы уверены?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Новый пользователь', new_admin_user_path %>
<%= link_to 'На главную', root_path %>